.detailContainer {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}

.detailTitleView {
  width: 100%;
  height: 30px;
  flex-direction: row;
  align-items: center;
  justify-content: flex-end;
  display: flex;
}
.pickerView {
  height: 30px;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  display: flex;
  margin-right: 20px;
  margin-bottom: 5px
}

.textName {
  font-size: 18px;
  font-weight: bold;
  margin: 15px;
}
.text {
  font-size: 13px;
  color: gray;
}

.pickerDateView{
  border: 1px solid gray;
  border-radius: 4px;
  display: flex;
  align-items: center;
  padding-left: 5px;
  padding-right: 5px;
}
.image{
  padding: 5px;
  width: 20px;
  height: 20px;
}
.detail{
  margin-top:5px;padding-left:15px;font-size:15px
}
.content{
  font-size:14px;
  color: gray
}
.cell{
  height: 44px;
  border-bottom: 1px solid gray;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  margin-left: 15px;
  margin-right: 15px;
  display: flex;
  background-color: beige;
  padding-left: 10px;
  padding-right: 10px;
}